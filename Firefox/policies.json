            "SponsoredSuggestions": false,
            "Locked": true
        },
        "SearchEngines": {
            "Default": "Google Web",
            "Add": [
                {
                    "Name": "Google Web",
                    "Description": "Google Search Web",
                    "Alias": "g",
                    "Method": "GET",
                    "URLTemplate": "https://www.google.com/search?udm=14&q={searchTerms}",
                    "SuggestURLTemplate": "https://suggestqueries.google.com/complete/search?client=chrome&q={searchTerms}",
                    "IconURL": "https://www.google.com/images/branding/googleg/1x/googleg_standard_color_128dp.png"
                },
                {
                    "Name": "YouTube",
                    "Description": "YouTube Search",
                    "Alias": "y",
                    "Method": "GET",
                    "URLTemplate": "https://www.youtube.com/results?search_query={searchTerms}",
                    "SuggestURLTemplate": "https://suggestqueries.google.com/complete/search?client=chrome&ds=yt&q={searchTerms}",
                    "IconURL": "https://www.youtube.com/s/desktop/c722ba88/img/logos/favicon_144x144.png"
                },
                {
                    "Name": "YouTube Music",
                    "Description": "Search YouTube Music",
                    "Alias": "ym",
                    "Method": "GET",
                    "URLTemplate": "https://music.youtube.com/search?q={searchTerms}",
                    "SuggestURLTemplate": "https://suggestqueries.google.com/complete/search?client=chrome&ds=yt&q={searchTerms}",
                    "IconURL": "https://music.youtube.com/img/favicon_144.png"
                },
                {
                    "Name": "ChatGPT",
                    "Description": "Search ChatGPT",
                    "Alias": "gpt",
                    "Method": "GET",
                    "URLTemplate": "https://chat.openai.com/?q={searchTerms}",
                    "SuggestURLTemplate": "https://suggestqueries.google.com/complete/search?client=chrome&q={searchTerms}",
                    "IconURL": "https://chat.openai.com/favicon.ico"
                },
                {
                    "Name": "Brave Search",
                    "Description": "Search via Brave",
                    "Alias": "br",
                    "Method": "GET",
                    "URLTemplate": "https://search.brave.com/search?q={searchTerms}",
                    "SuggestURLTemplate": "https://search.brave.com/api/suggest?q={searchTerms}",
                    "IconURL": "https://cdn.search.brave.com/serp/v3/_app/immutable/assets/apple-touch-icon.Bqba6l0U.png"
                },
                {
                    "Name": "Mullvad Leta",
                    "Description": "Privacy-first search by Mullvad",
                    "Alias": "ml",
                    "Method": "GET",
                    "URLTemplate": "https://leta.mullvad.net/search?q={searchTerms}&engine=google&country=us&language=en&lastUpdated=d",
                    "SuggestURLTemplate": "https://suggestqueries.google.com/complete/search?client=chrome&q={searchTerms}",
                    "IconURL": "https://mullvad.net/apple-touch-icon.png"
                }
            ]
        },
