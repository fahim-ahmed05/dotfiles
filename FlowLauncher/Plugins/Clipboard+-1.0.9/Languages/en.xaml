    <system:String x:Key="flowlauncher_plugin_clipboardplus_save_subtitle">Save this record to database</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_pin_title">Pin</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_pin_subtitle">Pin this record in list and database</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_unpin_title">Unpin</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_unpin_subtitle">Unpin this record in list and database</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_delete_title">Delete</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_delete_subtitle">Delete this record in both list and database</system:String>

    <system:String x:Key="flowlauncher_plugin_clipboardplus_unknown_app">unknown</system:String>

    <system:String x:Key="flowlauncher_plugin_clipboardplus_copy_to_clipboard" xml:space="preserve">Copy to clipboard: </system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_copy_to_clipboard_exception">Copy to clipboard failed!</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_text_data_invalid">Copied text is empty!</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_image_data_invalid">Error in copied image!</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_files_data_invalid">Copied files do not exist!</system:String>

    <!--  Preview  -->
    <system:String x:Key="flowlauncher_plugin_clipboardplus_words_count_prefix" xml:space="preserve">Words count: </system:String>

