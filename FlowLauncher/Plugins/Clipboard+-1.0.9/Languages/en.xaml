    <system:String x:Key="flowlauncher_plugin_clipboardplus_clear_invalid_subtitle">Clear invalid records in both list and database</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_clear_invalid_msg_subtitle">Clear {0} invalid records in both list and database!</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_clear_fail_msg_subtitle">There are no records that can be cleared!</system:String>

    <system:String x:Key="flowlauncher_plugin_clipboardplus_copy_title">Copy</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_copy_subtitle">Copy this record to clipboard</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_copy_plain_text_title">Copy as plain text</system:String>
    <system:String x:Key="flowlauncher_plugin_clipboardplus_copy_plain_text_subtitle">Copy this record as plain text</system:String>
