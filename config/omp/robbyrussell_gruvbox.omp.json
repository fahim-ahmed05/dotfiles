{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "foreground": "#a9b665",
          "style": "plain",
          "template": "\u279c",
          "type": "text"
        },
        {
          "foreground": "#83a598",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": " {{ .Path }}",
          "type": "path"
        },
        {
          "foreground": "#d3869b",
          "properties": {
            "branch_icon": ""
          },
          "style": "plain",
          "template": " <#928374>git:(</>{{ .HEAD }}<#928374>)</>",
          "type": "git"
        },
        {
          "foreground": "#ea6962",
          "style": "plain",
          "template": " \u2717",
          "type": "status"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 3
}
